##:
##: target: ~/.profile
##:

# GIT
export PATH="/usr/local/git/bin:$PATH"

# Local Gems
export PATH="$HOME/.gem/ruby/1.8/bin/:$PATH"



# MUST BE AT THE END!!!
# RVM (Ruby version manager) 
if [[ -s "$HOME/.rvm/scripts/rvm" ]]; then source "$HOME/.rvm/scripts/rvm"; fi
rvm use ruby-head > /dev/null
